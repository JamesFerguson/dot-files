export RE="http://re.dev"
export REX="http://rex-student.dev"
export REX_STUDENT="http://rex-student.dev"
export ED="http://eggspress-data.dev"
export EGGSPRESS_DATA="http://eggspress-data.dev"
export APP_SERVER="http://readingeggs.dev"

if [ -f "$rvm_path/scripts/rvm" ]; then
  source "$rvm_path/scripts/rvm"

  if [ -f ".rvmrc" ]; then
    source ".rvmrc"
  fi

  if [ -f ".ruby-version" ]; then
    rvm use `cat .ruby-version`
  fi

  if [ -f ".ruby-gemset" ]; then
    rvm gemset use --create `cat .ruby-gemset`
  fi
fi

# export CACHE="on"
